<?xml version="1.0" encoding="UTF-8"?>

<channel name="xfce4-notifyd" version="1.0">
  <property name="log-max-size-enabled" type="bool" value="true"/>
  <property name="applications" type="empty">
    <property name="known_applications" type="array">
      <value type="string" value="notify-send"/>
      <value type="string" value="Xfce4-notifyd settings"/>
    </property>
  </property>
  <property name="date-time-custom-format" type="string" value="%a %H:%M:%S"/>
  <property name="show-notifications-on" type="string" value="active-monitor"/>
  <property name="do-slideout" type="bool" value="true"/>
  <property name="do-fadeout" type="bool" value="false"/>
  <property name="notification-display-fields" type="string" value="full"/>
  <property name="initial-opacity" type="double" value="0.69999999999999996"/>
  <property name="min-width-enabled" type="bool" value="true"/>
  <property name="min-width" type="uint" value="200"/>
  <property name="notification-log" type="bool" value="true"/>
  <property name="log-max-size" type="uint" value="10000"/>
  <property name="expire-timeout" type="int" value="9"/>
</channel>
